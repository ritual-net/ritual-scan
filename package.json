    "plotly.js-dist-min": "^3.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-plotly.js": "^2.6.0",
    "react-window": "^2.1.2",

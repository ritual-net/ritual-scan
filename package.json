{
  "name": "etherscan-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "test": "playwright test",
    "test:watch": "playwright test --watch",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "docker:dev": "docker-compose --profile dev up --build",
    "docker:prod": "docker-compose --profile prod up -d --build",
    "docker:build": "docker build -t etherscan-clone:latest .",
    "docker:run": "docker run -p 3000:3000 etherscan-clone:latest",
    "docker:stop": "docker-compose down",
    "health": "curl -f http://localhost:3000/api/health || exit 1"
  },
  "dependencies": {
